  <DataSets>
    <DataSet Name="data">
      <Query>
        <DataSourceName>WideWorldImporters</DataSourceName>
        <CommandText>select distinct OrderYear, OrderMonth,AvgPriceInMonth, SumPriceInMonth from (
SELECT si.InvoiceDate, 
YEAR(si.InvoiceDate) OrderYear, 
MONTH(si.InvoiceDate) OrderMonth, 
AVG(sil.UnitPrice) over (partition by MONTH(si.InvoiceDate)) AvgPriceInMonth,
SUM(sil.Quantity * sil.UnitPrice) over (partition by MONTH(si.InvoiceDate)) SumPriceInMonth
FROM Sales.Invoices si
JOIN Sales.InvoiceLines sil on sil.InvoiceID = si.InvoiceID
WHERE si.ConfirmedDeliveryTime is not null)c
order by OrderYear, OrderMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="OrderYear">
          <DataField>OrderYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OrderMonth">
          <DataField>OrderMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AvgPriceInMonth">
          <DataField>AvgPriceInMonth</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SumPriceInMonth">
          <DataField>SumPriceInMonth</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
